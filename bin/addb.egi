#!/usr/bin/env egison
(load-file "../lib/egzact/utils.egi")
(load-file "../lib/egzact/filters.egi")

(define $main
  (lambda [$args]
    (print-each-line
      (initialize-args
        filter-plain
        []
        (append {"ifs=" "ofs="} (cdr args))
        (append stdin {(car args)})
        ))))
